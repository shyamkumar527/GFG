//POTD 9-1-24
vector <int> search(string pat, string txt){
    vector<int> v;
    int n=txt.size(),t=pat.size();
    for(int i=0;i<=n-t;i++){
        int f=1,a=0;
        for(int j=i;j<i+t;j++){
            if(txt[j]!=pat[a++]){
                f=0;
                break;
            }
        }
        if(f) v.push_back(i+1);
    }
    return v;
}
